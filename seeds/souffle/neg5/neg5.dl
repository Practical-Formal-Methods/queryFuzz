.type Set <: symbol

.decl X(node1:Set, node2:Set) // fuzzable relation
.output X()
.decl Y(node1:Set, node2:Set) // fuzzable relation
.output Y()
.decl Z(node1:Set, node2:Set) // fuzzable relation
.output Z()

// Negation
Y(a,b) :- X(a,b).
Y(a,c) :- X(a,b), Y(b,c).

//// As X does not contain ("g","h") Z is Y
Z(a,b) :- !X("g","h"), Y(a,b).

X("a","b").
X("b","c").
X("c","d").
