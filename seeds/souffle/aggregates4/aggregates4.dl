
.decl A(x:unsigned) // fuzzable relation
.decl C(x:unsigned) // fuzzable relation
.output C

A(1).
A(2).
A(3).
A(0xffffffff).


C(y)              :- y = max a : A(a).          // 0xffffffff
C(y)              :- y = min a : A(a).          // 1
C(y)              :- y = sum a : {A(a), a < 4}. // 6
C(to_unsigned(y)) :- y = count : {A(_)}.        // 4
